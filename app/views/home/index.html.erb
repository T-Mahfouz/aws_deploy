<main class="padding-top-75 ">
  <div id="slideshow" class="sliders">
    <div class="active-slide slide">
      <%= image_tag "Slider1Pictures/1.jpg", class:"image" %>
      <div class="slider-data-text">
        <div class="slider-heading">
          <%= t :world_renowned_experts %>
        </div>
        <div class="slider-text mt-2 mr-5 ml-5">
          <%= t  :slider_description %>
        </div>
        <button class="btns btns-submit mt-5">
          <%= t :ask_plus_one_expert %>
        </button>
      </div>

    </div>
    <div class="slide">
      <%= image_tag "Slider1Pictures/2.jpg", class:"image" %>
      <div class="slider-data-text">
        <div class="slider-heading">
          <%= t  :timely_consultations %>
        </div>
        <div class="slider-text mt-2 mr-5 ml-5">
          <%= t  :slider_description %>
        </div>
        <button class="btns btns-submit mt-5">
          <%= t :ask_plus_one_expert %>
        </button>
      </div>
    </div>
    <div class="slide">
      <%= image_tag "Slider1Pictures/3.jpg", class:"image" %>
      <div class="slider-data-text">
        <div class="slider-heading">
          <%= t :trustful_workflow %>
        </div>
        <div class="slider-text mt-2 mr-5 ml-5">
          <%= t  :slider_description %>
        </div>
        <button class="btns btns-submit mt-5">
          <%= t :ask_plus_one_expert %>
        </button>
      </div>
    </div>
  </div>
  <section class="services-section  <%= is_arabic? ? 'text-right-custom': '' %>" style="direction: ltr">
    <div class="services-container">
      <div class="services-item">
        <div class="services-icon">
          <i class="fa fa-ambulance fa-4x active"></i>
        </div>
        <div class="services-detail">
          <div class="services-heading heading-s">
            <%= t :world_renowned_experts %>
          </div>
          <div class="services-text">
            <%= t  :slider_description %>

          </div>
        </div>
      </div>
      <div class="services-item">
        <div class="services-icon">
          <i class="fa fa-ambulance fa-4x active"></i>
        </div>
        <div class="services-detail">
          <div class="services-heading heading-s">
            <%= t :trustful_workflow %>

          </div>
          <div class="services-text">
            <%= t  :slider_description %>
          </div>
        </div>
      </div>
      <div class="services-item">
        <div class="services-icon">
          <i class="fa fa-phone fa-rotate-270 fa-4x active"></i>
        </div>
        <div class="services-detail services-detail-border-none">
          <div class="services-heading heading-s">
            <%= t  :timely_consultations %>

          </div>
          <div class="services-text">
            <%= t  :slider_description %>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%= render partial: 'working_hours' %>
  <section class="quality-container <%= is_arabic? ? 'text-right-custom': '' %>">
    <div class="quality-background-container">
      <div class="quality-left-side">
          <div class="quality-item">
            <div class="quality-item-left">
              <div class="quality-item-left-icon">
                <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/icon-1-1.png" alt="icon" class="icon-photo">
              </div>
            </div>
              <div class="quality-item-right">
                <div class="quality-item-heading">
                  <%= t  :equality %>
                </div>
                <div class="quality-item-text gray-text">
                <%= t  :equality_text %>
                </div>
              </div>
          </div>
        <div class="quality-item">
          <div class="quality-item-left">
            <div class="quality-item-left-icon">
              <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/icon-2-1.png" alt="icon" class="icon-photo">
            </div>
          </div>
          <div class="quality-item-right">
            <div class="quality-item-heading">
              <%= t :security %>
            </div>
            <div class="quality-item-text gray-text">
            <%= t :security_text %>
            </div>
          </div>
        </div>
        <div class="quality-item">
          <div class="quality-item-left">
            <div class="quality-item-left-icon">
              <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/icon-3-1.png" alt="icon" class="icon-photo">
            </div>
          </div>
          <div class="quality-item-right">
            <div class="quality-item-heading">
             <%= t :quality %>
            </div>
            <div class="quality-item-text gray-text">
            <%= t :quality_text  %>
            </div>
          </div>
        </div>
        <div class="quality-item">
          <div class="quality-item-left">
            <div class="quality-item-left-icon">
              <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/icon-4.png" alt="icon" class="icon-photo">
            </div>
          </div>
          <div class="quality-item-right">
            <div class="quality-item-heading">
              <%= t :innovation %>
            </div>
            <div class="quality-item-text gray-text">
            <%= t :innoation_text %>
            </div>
          </div>
        </div>

      </div>
      <div class="quality-right-side">

      </div>
    </div>
  </section>
  <section class="counter-container <%= is_arabic? ? 'text-right-custom': '' %>">
    <div class="counter-back-image">
      <div class="counter-background-color-blue">
        <div class="counter-item">
          <div class="count counter-value">
            25
          </div>
          <div class="counter-name">
            <%= t  :oncologists %>
          </div>
        </div>
        <div class="counter-item">
          <div class="count counter-value">
            12
          </div>
          <div class="counter-name">
            <%= t   :neurologists %>
          </div>
        </div>
        <div class="counter-item">
          <div class="count counter-value">
           9
          </div>
          <div class="counter-name">
            <%= t  :orthopedics%>
          </div>
        </div>
        <div class="counter-item">
          <div class="count counter-value">
            5
          </div>
          <div class="counter-name">
           <%= t   :gynecologists %>
          </div>
        </div>
      </div>
    </div>
  </section>
  <%= render partial: 'qualified' %>
  <section class="reviews-container <%= is_arabic? ? 'text-right-custom': '' %>">
    <div class="reviews-inner-container">

       <%= render partial: 'form' %>

      <div class="reviews-right-side reviews-sides">
        <div class="reviews-heading-black heading-lg">
          Customer Reviews
        </div>
        <div class="reviews-review-container">
          <div class="reviews-review-left">
            <i class="fa fa-quote-right active fa-2x"></i>
          </div>
          <div class="reviews-review-right">
            <div class="reviews-review-right-top gray-text">
              Lorem ipsum dolor sit amet, consectetur adipisicing elit.
              Aliquam commodi consequatur deleniti distinctio, enim, eveniet perferendis porro quasi quo
              reprehenderit repudiandae sed sequi unde voluptatibus.
            </div>
            <div class="reviews-review-right-bottom">
              <div class="reviews-review-right-photo">
                <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/test-1.png" alt="photo-icon" class="profile-image">
              </div>
              <div>
              <div class="reviews-review-reviewer-name heading-md">
                Mario Hughes
              </div>
              <div class="reviews-review-reviewer-position">
                PEDIATRIC CENTER
              </div>
              </div>
            </div>
          </div>
        </div>
        <div class="reviews-review-container">
          <div class="reviews-review-left">
            <i class="fa fa-quote-right fa-2x active"></i>
          </div>
          <div class="reviews-review-right">
            <div class="reviews-review-right-top gray-text">
              Lorem ipsum dolor sit amet, consectetur adipisicing elit.
              Aliquam commodi consequatur deleniti distinctio, enim, eveniet perferendis porro quasi quo
              reprehenderit repudiandae sed sequi unde voluptatibus.
            </div>
            <div class="reviews-review-right-bottom">
              <div class="reviews-review-right-photo">
                <img src="https://bridge133.qodeinteractive.com/wp-content/uploads/2018/01/test-1.png" alt="photo-icon" class="profile-image">
              </div>
              <div>
              <div class="reviews-review-reviewer-name heading-md">
                Mario Hughes
              </div>
              <div class="reviews-review-reviewer-position">
                PEDIATRIC CENTER
              </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</main>
